@model IEnumerable<CityIndexVM>
    <a class="btn btn-primary" asp-action="Create">Create New City</a>
    <table class="table">
        <thead>
            <tr>
                <th>@Html.DisplayNameFor(model => model.Name)</th>
                <th>@Html.DisplayNameFor(model => model.PhoneCode)</th>
                <th>@Html.DisplayNameFor(model => model.PlateCode)</th>
                <th>@Html.DisplayNameFor(model => model.CountryName)</th>
                <th></th>

            </tr>
        </thead>
        <tbody>
            @foreach (var city in Model)
            {
                <tr class="gradeX">
                    <td>@Html.DisplayFor(modelItem => city.Name)</td>
                    <td>@Html.DisplayFor(modelItem => city.PhoneCode)</td>
                    <td>@Html.DisplayFor(modelItem => city.PlateCode)</td>
                    <td>@Html.DisplayFor(modelItem => city.CountryName)</td>
                    <td>
                        <a class="btn btn-danger" asp-action="Delete" asp-route-id="@city.Id">Delete</a>
                        <a class="btn btn-info" asp-action="Update" asp-route-id="@city.Id">Update</a>
                    </td>
                </tr>
            }


    </table>

